{
  "name": "vue-timeago3",
  "version": "2.1.1",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-timeago.umd.js",
  "module": "./dist/vue-timeago.es.js",
  "typings": "./dist_types/**/*.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-timeago.es.js",
      "require": "./dist/vue-timeago.umd.js"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrDeerly/vue-timeago3.git"
  },
  "bugs": {
    "url": "https://github.com/MrDeerly/vue-timeago3/issues"
  },
  "homepage": "https://mrdeerly.github.io/vue-timeago3/",
  "keywords": [
    "vue",
    "vue3",
    "timeago",
    "datefns",
    "date",
    "whileago",
    "currentTime",
    "distance",
    "time"
  ],
  "scripts": {
    "dev": "vite build && npm --prefix ./dist pack && npm --prefix examples run dev",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "serve": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "jest --config jest.config.ts",
    "lint": "eslint src --ext .ts",
    "release": "release-it"
  },
  "dependencies": {
    "date-fns": "^2.28.0"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^4.1.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.4.1",
    "jest": "^27.5.1",
    "prettier": "2.5.1",
    "release-it": "^14.12.4",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vitepress": "^0.22.0",
    "vue-eslint-parser": "^8.2.0",
    "vue-tsc": "^0.31.4"
  }
}
